<?php

namespace App\Http\Controllers;

class WelcomeController extends Controller
{
public function index()
{
    $breadcrum =(object) [
        'title' =>'selamat Datang',
        'list'=> ['Home','Welcome']
    ];
    $activeMenu = 'dashboard';

    return view ('Welcome',['breadcrumb'=>$breadcrum, 'active_menu'=>$activeMenu]);
}
}
